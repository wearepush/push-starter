{
  "moduleFileExtensions": [
    "jsx",
    "js",
    "json"
  ],
  "moduleNameMapper": {
    "^.*\\.(css|scss|sass)$": "identity-obj-proxy"
  },
  "moduleDirectories": ["node_modules"],
  "testEnvironment": "jsdom",
  "coverageDirectory": "./coverage/",
  "collectCoverage": true,
  "setupFiles": [
    "./enzyme"
  ],
  "notify": true,
  "transform": {
    "^.+\\.js$": "babel-jest"
  }
}
