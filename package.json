{
  "name": "redux-starter",
  "description": "React 16, React Router 4, Redux, Webpack 4, Express, Server side rendering",
  "version": "1.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wearepush/redux-starter"
  },
  "homepage": "https://wearepush-redux-starter.herokuapp.com",
  "scripts": {
    "start": "npm run dev",
    "dev": "run-p dev:client:build dev:server:prepare dev:server:build dev:server:delay",
    "dev:server:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "dev:server:build": "webpack --config ./webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
    "dev:server:delay": "run-s delay dev:server:start",
    "dev:server:start": "nodemon ./src/server/start.babel.js --watch ./static",
    "dev:client:build": "babel-node ./webpack/webpack-dev-server.js",
    "delay": "node ./node_modules/npm-delay 15000",
    "prod": "run-s prod:client:build prod:server:build prod:server:start",
    "prod:client:build": "NODE_ENV=production webpack --config ./webpack/webpack.config.client.production.babel.js --colors --display-error-details",
    "prod:server:build": "NODE_ENV=production webpack --config ./webpack/webpack.config.server.production.babel.js --colors --display-error-details",
    "prod:server:start": "NODE_ENV=production node ./src/server/start.babel.js",
    "heroku-postbuild": "run-p prod:client:build prod:server:build",
    "lint": "eslint -c .eslintrc src",
    "test": "NODE_ENV=test jest --config jest.config.json",
    "test:watch": "NODE_ENV=test jest --watch --config jest.config.json"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "jest",
    "enzyme",
    "css-modules",
    "react-router",
    "react-router-dom"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/register": "^7.0.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bourbon": "^5.1.0",
    "bourbon-neat": "^3.0.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "current-device": "^0.7.8",
    "detect-browser": "^3.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-winston": "^3.0.0",
    "history": "^4.7.2",
    "lodash": "^4.17.10",
    "lru-memoize": "^1.0.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-ga": "^2.5.3",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.4",
    "react-redux": "^5.0.7",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redbox-react": "^1.6.0",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-starter-ui": "^1.2.17",
    "reselect": "^3.0.1",
    "serialize-javascript": "^1.5.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.2",
    "browserslist": "^4.1.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.80.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "mini-css-extract-plugin": "^0.4.2",
    "nock": "^9.6.1",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "npm-delay": "^1.0.4",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "universal-webpack": "^0.6.6",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-dev-server": "^3.1.7",
    "webpack-hot-middleware": "^2.23.1",
    "webpack-merge": "^4.1.4"
  },
  "engines": {
    "node": "^8.9.1"
  }
}
