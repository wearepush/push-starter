{
  "name": "redux-starter",
  "description": "React 16, React Router 4, Redux, Webpack 4, Express, Server side rendering",
  "version": "1.0.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wearepush/redux-starter"
  },
  "homepage": "https://wearepush-redux-starter.herokuapp.com",
  "scripts": {
    "start": "npm run dev",
    "dev": "run-p dev:client:build dev:server:prepare dev:server:build dev:server:delay",
    "dev:server:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "dev:server:build": "UNIVERSAL_WEBPACK_CSS_LOADER_V2=true webpack --config ./webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
    "dev:server:delay": "run-s delay dev:server:start",
    "dev:server:start": "nodemon ./src/server/start.babel.js --watch ./static",
    "dev:client:build": "babel-node ./webpack/webpack-dev-server.js",
    "delay": "node ./node_modules/npm-delay 15000",
    "prod": "run-s prod:build prod:start",
    "prod:start": "npm-run-all prod:server:start",
    "prod:build": "run-p prod:client:build prod:server:build",
    "prod:client:build": "NODE_ENV=production webpack --config ./webpack/webpack.config.client.production.babel.js --colors --display-error-details",
    "prod:server:build": "NODE_ENV=production UNIVERSAL_WEBPACK_CSS_LOADER_V2=true webpack --config ./webpack/webpack.config.server.production.babel.js --colors --display-error-details",
    "prod:server:start": "NODE_ENV=production node ./src/server/start.babel.js",
    "heroku-postbuild": "run-p prod:build",
    "lint": "eslint -c .eslintrc src",
    "test": "NODE_ENV=test jest --config jest.config.json",
    "test:watch": "NODE_ENV=test jest --watch --config jest.config.json"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "jest",
    "enzyme",
    "css-modules",
    "react-router",
    "react-router-dom"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/register": "^7.0.0",
    "axios": "^0.18.0",
    "bourbon": "^5.1.0",
    "bourbon-neat": "^3.0.1",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "connected-react-router": "^6.3.2",
    "current-device": "^0.8.0",
    "detect-browser": "^4.1.0",
    "express": "^4.16.4",
    "express-winston": "^3.1.0",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "lru-memoize": "^1.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-ga": "^2.5.7",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.8.0",
    "react-redux": "^6.0.1",
    "react-router": "^4.4.0-beta.6",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.4.0-beta.6",
    "redbox-react": "^1.6.0",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0",
    "redux-starter-ui": "^2.0.3",
    "reselect": "^4.0.0",
    "serialize-javascript": "^1.6.1",
    "serve-favicon": "^2.5.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-runtime": "^6.26.0",
    "browserslist": "^4.4.2",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "dotenv": "^7.0.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.94.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "mini-css-extract-plugin": "^0.5.0",
    "nock": "^10.0.6",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "npm-delay": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "universal-webpack": "^0.6.11",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.6.1",
    "webpack-dev-server": "^3.2.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.2.1"
  },
  "engines": {
    "node": "^10.15.1"
  }
}
