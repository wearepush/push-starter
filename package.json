  "homepage": "",
  "keywords": [],
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start-prod\"",
    "start-local": "concurrently --kill-others \"better-npm-run start-local\"",
    "start-prod": "better-npm-run start-prod",
    "build": "better-npm-run build",
    "build-local": "better-npm-run build-local",
    "lint": "eslint -c .eslintrc src",
    "postinstall": "npm run build",
    "start-dev": "better-npm-run start-dev",
    "test": "jest",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\""
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "APIHOST": "wearepush-redux-starter.herokuapp.com",
        "APIPORT": "",
        "HOST": "wearepush-redux-starter.herokuapp.com"
      }
    },
    "start-local": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIHOST": "wearepush-redux-starter.herokuapp.com",
        "APIPORT": "",
        "HOST": "localhost"
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 8080,
        "APIHOST": "wearepush-redux-starter.herokuapp.com",
        "APIPORT": ""
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 8008,
        "APIHOST": "wearepush-redux-starter.herokuapp.com",
        "APIPORT": ""
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "APIHOST": "wearepush-redux-starter.herokuapp.com",
        "APIPORT": ""
      }
    },
    "build-local": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "APIHOST": "wearepush-redux-starter.herokuapp.com",
        "APIPORT": ""
      }
    }
  },
