{
  "name": "redux-starter",
  "version": "1.0.3",
  "description": "React 16, React Router 4, Redux, Webpack 4, Express, Server side rendering",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "jest",
    "enzyme",
    "css-modules",
    "react-router",
    "react-router-dom"
  ],
  "homepage": "https://wearepush-starter.herokuapp.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/wearepush/push-starter"
  },
  "license": "MIT",
  "scripts": {
    "commit": "npx git-cz",
    "delay": "node ./node_modules/npm-delay 15000",
    "dev": "run-p dev:client:build dev:server:prepare dev:server:build dev:server:delay",
    "dev:client:build": "babel-node ./webpack/webpack-dev-server.js",
    "dev:server:build": "UNIVERSAL_WEBPACK_CSS_LOADER_V3=true webpack --config ./webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
    "dev:server:delay": "run-s delay dev:server:start",
    "dev:server:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "dev:server:start": "nodemon ./src/server/start.babel.js --watch ./static --watch ./src/server",
    "heroku-postbuild": "run-p prod:build",
    "lint:fix": "cross-env NODE_ENV=production eslint --config .eslintrc.js src --ignore-pattern !.eslintrc.js --fix",
    "lint:js": "cross-env NODE_ENV=production eslint --config .eslintrc.js src --ignore-pattern !.eslintrc.js",
    "lint:package": "sort-package-json",
    "prod": "run-s prod:build prod:start",
    "prod:build": "run-p prod:client:build prod:server:build",
    "prod:client:build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.client.production.babel.js --colors --display-error-details",
    "prod:server:build": "cross-env NODE_ENV=production UNIVERSAL_WEBPACK_CSS_LOADER_V3=true webpack --config ./webpack/webpack.config.server.production.babel.js --colors --display-error-details",
    "prod:server:start": "cross-env NODE_ENV=production node ./src/server/start.babel.js",
    "prod:start": "npm-run-all prod:server:start",
    "start": "npm run dev",
    "test": "cross-env NODE_ENV=test jest --config jest.config.json",
    "test:watch": "cross-env NODE_ENV=test jest --watch --config jest.config.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix"
    ],
    "package.json": [
      "npm run lint:package"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "IE 10"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/register": "^7.13.8",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "connected-react-router": "^6.9.1",
    "cookie-parser": "^1.4.5",
    "current-device": "^0.10.0",
    "detect-browser": "^5.2.0",
    "express": "^4.17.1",
    "express-winston": "^4.1.0",
    "history": "^4.7.2",
    "husky": "^5.1.3",
    "lodash": "^4.17.21",
    "lru-memoize": "^1.1.0",
    "newrelic": "^7.1.3",
    "prop-types": "^15.7.2",
    "push-ui": "^1.0.15",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-ga": "^2.7.0",
    "react-helmet-async": "^1.0.9",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redbox-react": "^1.6.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.7",
    "reselect": "^4.0.0",
    "serialize-javascript": "^3.0.0",
    "serve-favicon": "^2.5.0",
    "universal-webpack": "^0.6.12",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@hot-loader/react-dom": "^17.0.1",
    "autoprefixer": "^9.7.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.2.2",
    "babel-runtime": "^6.26.0",
    "browserslist": "^4.16.3",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "css-loader": "^3.5.2",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsdoc": "^23.0.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.3.0",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^0.9.0",
    "nock": "^12.0.3",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.7",
    "npm-delay": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "^9.0.1",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^8.0.2",
    "sort-package-json": "^1.49.0",
    "style-loader": "^1.1.4",
    "url-loader": "^4.1.1",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  },
  "engines": {
    "node": "^12.13.1"
  }
}
