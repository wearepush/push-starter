{
  "name": "redux-starter",
  "version": "1.0.3",
  "description": "React 17, React Router 4, Redux, Webpack 4, Express, Server side rendering",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "jest",
    "enzyme",
    "css-modules",
    "react-router",
    "react-router-dom"
  ],
  "homepage": "https://wearepush-starter.herokuapp.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/wearepush/push-starter"
  },
  "license": "MIT",
  "scripts": {
    "commit": "npx git-cz",
    "start": "npm run production",
    "dev": "npm-run-all development:client:rendering-service:build:prepare development",
    "development": "npm-run-all --parallel development:client:build development:client:rendering-service:build development:client:rendering-service",
    "development:client:rendering-service:build:prepare": "universal-webpack --settings ./config/universal-webpack-settings.json prepare",
    "development:client:build": "webpack serve --hot --config config/webpack.config.client.development.babel.js",
    "development:client:rendering-service:build": "webpack --mode development --config ./config/webpack.config.server.development.babel.js --watch --color --stats-errors",
    "development:client:rendering-service": "nodemon ./src/server/start.babel.js --watch ./src/server --watch ./build/server --no-update-notifier",
    "production": "npm-run-all production:client:build production:client:rendering-service:build production:services",
    "production:client:build": "webpack --mode production --color --stats-errors --config ./config/webpack.config.client.production.babel.js",
    "production:client:rendering-service:build": "webpack --mode production --color --stats-errors --config ./config/webpack.config.server.production.babel.js",
    "production:api": "cross-env NODE_ENV=production node ./api/index.entry.js",
    "production:client:rendering-service": "cross-env NODE_ENV=production node ./rendering-service/index.entry.js",
    "production:services": "npm-run-all --parallel production:api production:client:rendering-service",
    "production:build": "npm-run-all production:client:build production:client:rendering-service:build",
    "heroku-postbuild": "npm run production:build",
    "lint:fix": "cross-env NODE_ENV=production eslint --config .eslintrc.js src --ignore-pattern !.eslintrc.js --fix",
    "lint:js": "cross-env NODE_ENV=production eslint --config .eslintrc.js src --ignore-pattern !.eslintrc.js",
    "lint:package": "sort-package-json",
    "test": "cross-env NODE_ENV=test jest --config jest.config.json",
    "test:watch": "cross-env NODE_ENV=test jest --watch --config jest.config.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix"
    ],
    "package.json": [
      "npm run lint:package"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "IE 10"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/register": "^7.13.8",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "connected-react-router": "^6.9.1",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.10.0",
    "current-device": "^0.10.2",
    "detect-browser": "^5.2.0",
    "express": "^4.17.1",
    "express-winston": "^4.1.0",
    "history": "^4.7.2",
    "husky": "4.2.5",
    "lodash": "^4.17.21",
    "lru-memoize": "^1.1.0",
    "newrelic": "^7.1.3",
    "prop-types": "^15.7.2",
    "push-ui": "^1.0.15",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-ga": "^3.3.0",
    "react-helmet-async": "^1.0.9",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redbox-react": "^1.6.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.7",
    "reselect": "^4.0.0",
    "serialize-javascript": "^5.0.1",
    "serve-favicon": "^2.5.0",
    "serverless-functions": "^0.4.2",
    "universal-webpack": "^0.6.15",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@hot-loader/react-dom": "^17.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.0-beta.1",
    "autoprefixer": "^10.2.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-runtime": "^6.26.0",
    "browserslist": "^4.16.3",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.9",
    "nock": "^13.0.11",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.7",
    "npm-delay": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "react-dev-utils": "^11.0.4",
    "react-refresh": "^0.9.0",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^11.0.1",
    "sort-package-json": "^1.49.0",
    "style-loader": "^2.0.0",
    "url": "^0.11.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.26.3",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "engines": {
    "node": "^14.0.0"
  }
}
