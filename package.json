{
  "name": "redux-starter",
  "description": "",
  "version": "1.0.0-beta.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wearepush/redux-starter"
  },
  "homepage": "https://wearepush-redux-starter.herokuapp.com",
  "scripts": {
    "start": "npm run dev",
    "dev": "run-p dev:client:build dev:server:prepare dev:server:build dev:server:delay",
    "dev:server:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "dev:server:build": "webpack --config ./webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
    "dev:server:delay": "run-s delay dev:server:start",
    "dev:server:start": "nodemon ./src/server/start.babel.js --watch ./static",
    "dev:client:build": "babel-node ./webpack/webpack-dev-server.js",
    "delay": "node ./node_modules/npm-delay 15000",
    "prod": "npm-run-all prod:client:build prod:server:build prod:server:start",
    "prod:client:build": "NODE_ENV=production webpack -p --config ./webpack/webpack.config.client.production.babel.js --colors --display-error-details",
    "prod:server:build": "NODE_ENV=production webpack --config ./webpack/webpack.config.server.production.babel.js --colors --display-error-details",
    "prod:server:start": "NODE_ENV=production node ./src/server/start.babel.js",
    "heroku-postbuild": "npm-run-all prod:client:build prod:server:build",
    "lint": "eslint -c .eslintrc src",
    "test": "NODE_ENV=test jest --config jestconfig.json",
    "watch:test": "NODE_ENV=test jest --watch --config jestconfig.json"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "jest",
    "enzyme",
    "css-modules",
    "react-router",
    "react-router-dom"
  ],
  "dependencies": {
    "axios": "^0.17.0",
    "babel-polyfill": "^6.26.0",
    "bourbon": "^4.3.4",
    "bourbon-neat": "^1.9.0",
    "classnames": "^2.2.5",
    "compression": "^1.7.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "global": "^4.3.2",
    "history": "^4.7.2",
    "identity-obj-proxy": "^3.0.0",
    "immutable": "^3.8.2",
    "lodash": "^4.17.4",
    "lru-memoize": "^1.0.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.8",
    "redbox-react": "^1.5.0",
    "redux": "^3.7.2",
    "redux-form": "^7.1.2",
    "redux-immutablejs": "^0.0.8",
    "serve-favicon": "^2.4.5"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-flow-runtime": "^0.11.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.17",
