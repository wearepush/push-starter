{
  "name": "redux-starter",
  "description": "",
  "version": "0.1.5",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wearepush/redux-starter"
  },
  "homepage": "https://wearepush-redux-starter.herokuapp.com",
  "scripts": {
    "start": "npm run dev",
    "dev": "run-p dev:client:build dev:server:prepare dev:server:build dev:server:delay",
    "dev:server:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "dev:server:build": "webpack --config ./webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
    "dev:server:delay": "run-s delay dev:server:start",
    "dev:server:start": "nodemon ./src/server/start.babel.js --watch ./static",
    "dev:client:build": "babel-node ./webpack/webpack-dev-server.js",
    "delay": "node ./node_modules/npm-delay 15000",
    "prod": "npm-run-all prod:client:build prod:server:build prod:server:start",
    "prod:client:build": "NODE_ENV=production webpack -p --config ./webpack/webpack.config.client.production.babel.js --colors --display-error-details",
    "prod:server:build": "NODE_ENV=production webpack --config ./webpack/webpack.config.server.production.babel.js --colors --display-error-details",
    "prod:server:start": "NODE_ENV=production node ./src/server/start.babel.js",
    "heroku-postbuild": "npm-run-all prod:client:build prod:server:build",
    "lint": "eslint -c .eslintrc src",
    "test": "jest --config jestconfig.json",
    "watch:test": "jest --watch --config jestconfig.json"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "jest",
    "enzyme",
    "css-modules"
  ],
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "compression": "^1.7.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "global": "^4.3.2",
    "identity-obj-proxy": "^3.0.0",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "lru-memoize": "^1.0.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-connect": "^5.1.0",
    "redux-form": "^7.0.4",
    "redux-immutablejs": "^0.0.8",
    "semantic-ui-react": "^0.73.0",
    "serve-favicon": "^2.4.4",
    "superagent": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-flow-runtime": "^0.11.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "dotenv-webpack": "^1.5.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "expect": "^21.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.54.1",
    "flow-runtime": "^0.14.0",
    "jest": "^21.1.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.0",
    "npm-delay": "^1.0.4",
    "npm-run-all": "^4.1.1",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^15.6.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "universal-webpack": "^0.4.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": "^7.1.0",
    "yarn": "^1.0.2"
  }
}
