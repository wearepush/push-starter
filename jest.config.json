{
  "moduleFileExtensions": [
    "jsx",
    "js",
    "json"
  ],
  "moduleNameMapper": {
    "^.*\\.(css|scss)$": "identity-obj-proxy"
  },
  "moduleDirectories": [
    "node_modules"
  ],
  "testEnvironment": "jsdom",
  "coverageDirectory": "./coverage/",
  "collectCoverage": true,
  "setupFiles": [
    "<rootDir>/src/setupTests.js"
  ],
  "transform": {
    "^.+\\.js$": "babel-jest"
  }
}