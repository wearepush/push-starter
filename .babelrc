{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    ["transform-runtime"],
    ["add-module-exports"],
    ["transform-decorators-legacy"],
    ["transform-flow-strip-types"]
  ],
  "env": {
    "development": {
      "plugins": [
        "typecheck"
      ]
    }
  }
}
